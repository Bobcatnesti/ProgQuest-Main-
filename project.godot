; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ProgQuest - Mobile"
run/main_scene="res://MainMenu.tscn"
config/features=PackedStringArray("4.3", "Mobile")

[autoload]

BgMusic="*res://MainMenu.tscn"
SettingScene="*res://SettingScene.tscn"
Global="*res://global.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
InteractionManager="*res://interaction/InteractionManager.tscn"
PlayerManager="*res://00_Globals/global_player_manager.gd"
NavigationManager="*res://00_Globals/navigation_manager.gd"

[dialogic]

directories/dch_directory={
"Npc": "res://Character(Dialogic)/Npc.dch",
"Try": "res://Try.dch"
}
directories/dtl_directory={
"AfterReading": "res://timeline/AfterReading.dtl",
"AfterSlimeQuest": "res://timeline/AfterSlimeQuest.dtl",
"Start": "res://Dialogue/Start.dtl",
"npc": "res://timeline/npc.dtl"
}
variables={
"Respond(Travelling)": ""
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
glossary/default_case_sensitive=true
layout/style_list=["res://STYLEDAW.tres"]
layout/default_style="res://STYLEDAW.tres"
portraits/default_portrait=""

[display]

window/size/viewport_width=2436
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://Animation/": "red",
"res://Miscs Sprite/": "gray",
"res://Resources/": "teal",
"res://Scene/": "red",
"res://Sprite/": "red",
"res://interaction/": "gray",
"res://jsonFile/": "blue"
}

